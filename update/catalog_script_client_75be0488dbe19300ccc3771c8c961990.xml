        <script><![CDATA[function onSubmit() {
	//Type appropriate comment here, and begin script below
	var ajax = new GlideAjax('GetCapacities');
	ajax.addParam('sysparm_name','getcapacity');
	ajax.addParam ('training', g_form.getValue('training'));
	ajax.getXML(autoPop);
	function autoPop(response)
	{
		var answer = response.responseXML.documentElement.getAttribute("answer");
		var cpt=g_form.getValue('inscriptions_nombre');
		if (answer!=cpt){
			cpt= answer-cpt;
			g_form.setValue("inscriptions_nombre", cpt);
			g_form.addInfoMessage(" the user is registered ");
		}
		else
			{
			g_form.addInfoMessage("The maximum number of places reached ");
		    }
	}
}]]></script>
